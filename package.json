{
  "name": "@netcracker/qubership-apihub-build-task-consumer",
  "version": "3.1.3",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "development:link": "npm link && npm link @netcracker/qubership-apihub-api-processor",
    "development:unlink": "npm unlink && npm unlink @netcracker/qubership-apihub-api-processor",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=3800 --max-semi-space-size=32 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:fixme": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "update-lock-file": "update-lock-file @netcracker"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@netcracker/qubership-apihub-api-processor": "4.1.2-bugfix-index-file-is-missing-export.20250814131036",
    "@sinclair/typebox": "^0.32.1",
    "adm-zip": "^0.5.10",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "openapi-types": "^12.1.0",
    "js-yaml": "4.1.0",
    "jszip": "^3.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@netcracker/qubership-apihub-npm-gitflow": "3.1.0",
    "@types/adm-zip": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.5",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "axios": "^1.6.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "parse-multipart-data": "^1.5.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^nimma/legacy$": "<rootDir>/../node_modules/nimma/dist/legacy/cjs/index.js",
      "^nimma/(.*)": "<rootDir>/../node_modules/nimma/dist/cjs/$1"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
